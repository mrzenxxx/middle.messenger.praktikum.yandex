{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "max-len": [2, 120],
        "import/prefer-default-export": "off"
    }
}
