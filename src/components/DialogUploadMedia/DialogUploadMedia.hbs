{{#Dialog open=isOpen}}
<form method="dialog">
    <h4>Загрузить файл</h4>
    {{{ Button
        label="Выбрать файл"
        type="button"
        style="secondary"
        onClick=onClose
        }}}
    {{{ ErrorLine error=error ref="errorMessage"}}}
    <div>
        {{{ Button
        label="Загрузить"
        type="submit"
        style="primary"
        onClick=onSubmit
        }}}
        {{{ Button
        label="Отменить"
        type="button"
        style="secondary"
        onClick=onClose
        }}}
    </div>
</form>
{{/Dialog}}
