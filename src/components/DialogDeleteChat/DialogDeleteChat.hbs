{{#Dialog open=isOpen}}
    {{#Form type="dialog"}}
        <h4>Вы уверены?</h4>
        {{{ApiErrorMessage}}}
        <div>
            {{{ Button
            label="Удалить чат"
            type="submit"
            style="primary"
            onClick=onDeleteChat
            }}}
            {{{ Button
            label="Отменить"
            type="button"
            style="secondary"
            onClick=onClose
            }}}
        </div>
    {{/Form}}
{{/Dialog}}
